{
  "expo": {
    "name": "Claudyne",
    "slug": "claudyne-platform",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "description": "Plateforme Ã©ducative IA pour l'excellence scolaire au Cameroun - Cours, Quiz, Mentor IA",
    "primaryColor": "#2E7D32",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E7D32"
    },
    "privacy": "public",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.claudyne.app",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.claudyne.app",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2E7D32"
