        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "claudyne"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.claudyne.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5522402b-87e8-4f92-9dff-380bc8850fd5"
      }
    },
    "owner": "aurelgroup",
    "plugins": [
      "expo-font"
    ],
    "sdkVersion": "54.0.0"
  }
}
